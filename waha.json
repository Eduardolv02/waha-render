{
  "sessionStorage": {
    "type": "supabase",
    "url": "https://${SUPABASE_PROJECT_ID}.supabase.co",
    "key": "${SUPABASE_SERVICE_ROLE_KEY}",
    "options": {
      "bucket": "waha",
      "path": "sessions"
    }
  },
  "database": {
    "type": "postgres",
    "url": "${SUPABASE_DATABASE_URL}"
  },
  "logLevel": "info",
  "browser": {
    "type": "playwright",
    "channel": "chromium",
    "args": [
      "--disable-dev-shm-usage",
      "--no-sandbox",
      "--single-process",
      "--no-zygote",
      "--renderer-process-limit=1"
    ]
  }
}
